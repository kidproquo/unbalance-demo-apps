    image: unbalanced-demo-apps:local

  approach-cnn:
    build: .
    image: unbalanced-demo-apps:local

  approach-fft:
    build: .
    image: unbalanced-demo-apps:local

  approach-rfc:
    build: .
    image: unbalanced-demo-apps:local
